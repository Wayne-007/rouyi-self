{
  "files": [],
  "references": [{ "path": "./tsconfig.app.json" }, { "path": "./tsconfig.node.json" }],
  "compilerOptions": {
    "types": ["element-plus/global"],
    // 配置别名
    "paths": {
      "baseUrl": "./",
      "@/*": ["src/*"],
      "/#/*": ["types/*"]
    }
  },
  "include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*.tsx", "src/**/*.vue", "types/**/*.d.ts", "types/**/*.ts", "build/**/*.ts", "build/**/*.d.ts", "vite.config.ts"],
  "exclude": ["node_modules", "tests/server/**/*.ts", "dist", "**/*.js"]
}
